<h1>Listing modeles</h1>

<table>
  <tr>
    <th>Nom</th>
    <th>Desc</th>
    <th>Weburl</th>
    <th>Picture filename</th>
    <th>Picture content type</th>
    <th>Picture file size</th>
    <th>Picture updated at</th>
  </tr>

<% @modeles.each do |modele| %>
  <tr>
    <td><%=h modele.nom %></td>
    <td><%=h modele.desc %></td>
    <td><%=h modele.weburl %></td>
    <td><%=h modele.picture_filename %></td>
    <td><%=h modele.picture_content_type %></td>
    <td><%=h modele.picture_file_size %></td>
    <td><%=h modele.picture_updated_at %></td>
    <td><%= link_to 'Show', modele %></td>
    <td><%= link_to 'Edit', edit_modele_path(modele) %></td>
    <td><%= link_to 'Destroy', modele, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New modele', new_modele_path %>